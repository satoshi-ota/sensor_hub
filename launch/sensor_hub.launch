<?xml version="1.0"?>

<launch>
    <arg name="port" default="/dev/ttyUSB0"/>

    <node name="sensor_hub_node" pkg="sensor_hub" type="sensor_hub_node">
        <param name="port" value="$(arg port)" />
    </node>
    <node pkg="rqt_reconfigure" type="rqt_reconfigure" name="rqt_reconfigure"/>
</launch>
